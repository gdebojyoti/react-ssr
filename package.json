    "babel-loader": "^8.0.6",
    "express": "^4.17.1",
    "nodemon": "^1.19.3",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-router-dom": "^5.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
